[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/home/dms_bulk_django
ExecStart=/home/dms_bulk_django/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          dms_bulk_django.wsgi:application

[Install]
WantedBy=multi-user.target
